<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginBottom="10dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:orientation="vertical">
        <com.opensis.others.customtext.Custom_Text_View_SemiBold
            android:id="@+id/tvNotice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/large_text"
            android:textColor="@color/black"
            android:text="C.W 1 Assignment Grades due on January 10, 2023"></com.opensis.others.customtext.Custom_Text_View_SemiBold>
        <com.opensis.others.customtext.Custom_Text_View
            android:layout_marginTop="5dp"
            android:id="@+id/tvNoticeDetails"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/medium_text"
            android:textColor="@color/black"
            android:text="C.W 1 Assignment Grades due on January 10, 2023.C.W 1 Assignment Grades due on January 10, 2023.C.W 1 Assignment Grades due on January 10, 2023.C.W 1 Assignment Grades due on January 10, 2023"></com.opensis.others.customtext.Custom_Text_View>
    </LinearLayout>


</LinearLayout>