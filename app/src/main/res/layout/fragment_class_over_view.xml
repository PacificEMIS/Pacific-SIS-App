                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Course"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvCourse"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:layout_marginTop="2dp"
                                android:textColor="@color/black"
                               ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="20dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Calendar"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvCalendar"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:layout_marginTop="2dp"
                                android:textColor="@color/black"
                                ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="20dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Credit Hours"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvCredtHours"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:layout_marginTop="2dp"
                                android:textColor="@color/black"
                                ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="20dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Attendance Category"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvAttenDanceCategory"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:layout_marginTop="2dp"
                                android:textColor="@color/black"
                                ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Subject"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvSubject"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:layout_marginTop="2dp"
                                android:textColor="@color/black"
                                ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="20dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Grad Scale"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvGradeScale"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:layout_marginTop="2dp"
                                android:textColor="@color/black"
                             ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="20dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Seats"></com.opensis.others.customtext.Custom_Text_View>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp">
                                <com.opensis.others.customtext.Custom_Text_View_Bold
                                    android:id="@+id/tvSeats"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/large_text"
                                    android:textColor="@color/black"
                                    ></com.opensis.others.customtext.Custom_Text_View_Bold>
                                <View
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:layout_width="1dp"
                                    android:layout_height="20dp"
                                    android:background="@color/black"></View>
                                <com.opensis.others.customtext.Custom_Text_View_Bold
                                    android:id="@+id/tvAvailableSeat"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/large_text"
                                    android:textColor="@color/green"
                                    ></com.opensis.others.customtext.Custom_Text_View_Bold>
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="20dp"
                android:background="@color/grey"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="20dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/imgCourseWeighted"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/check_black_48dp"></ImageView>
                    <com.opensis.others.customtext.Custom_Text_View
                        android:id="@+id/tvWeightedCourse"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/small_text"
                        android:textColor="@color/black"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        android:text="Course is Weighted"></com.opensis.others.customtext.Custom_Text_View>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/imgUseStandard"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/check_black_48dp"></ImageView>
                    <com.opensis.others.customtext.Custom_Text_View
                        android:id="@+id/tvUseStandard"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/small_text"
                        android:textColor="@color/black"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        android:text="Use Standards"></com.opensis.others.customtext.Custom_Text_View>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/imgAffectClassRank"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/check_black_48dp"></ImageView>
                    <com.opensis.others.customtext.Custom_Text_View
                        android:id="@+id/tvAffectClassRank"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/small_text"
                        android:textColor="@color/black"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        android:text="Affect Class Rank"></com.opensis.others.customtext.Custom_Text_View>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/imgAffectHonorRoll"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/check_black_48dp"></ImageView>
                    <com.opensis.others.customtext.Custom_Text_View
                        android:id="@+id/tvAffectHonorRoll"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/small_text"
                        android:textColor="@color/black"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        android:text="Affects Honor Roll"></com.opensis.others.customtext.Custom_Text_View>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/imgOnlineClass"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/check_black_48dp"></ImageView>
                    <com.opensis.others.customtext.Custom_Text_View
                        android:id="@+id/tvOnlineClass"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/small_text"
                        android:textColor="@color/black"
                        android:layout_marginLeft="10dp"
                        android:layout_gravity="center"
                        android:text="Online Classroom"></com.opensis.others.customtext.Custom_Text_View>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="20dp"
                android:background="@color/grey"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="20dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.opensis.others.customtext.Custom_Text_View
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/medium_text"
                        android:textColor="@color/bar"
                        android:text="Standard Grade Scale"></com.opensis.others.customtext.Custom_Text_View>
                    <com.opensis.others.customtext.Custom_Text_View_Bold
                        android:id="@+id/tvMainStandardGradeScale"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/large_text"
                        android:layout_marginTop="2dp"
                        android:textColor="@color/black"
                       ></com.opensis.others.customtext.Custom_Text_View_Bold>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.opensis.others.customtext.Custom_Text_View
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/medium_text"
                        android:textColor="@color/bar"
                        android:text="Online Classroom URL"></com.opensis.others.customtext.Custom_Text_View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp">
                        <com.opensis.others.customtext.Custom_Text_View_Bold
                            android:id="@+id/tvOnlineUrl"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/large_text"
                            android:textColor="@color/text_color"
                           ></com.opensis.others.customtext.Custom_Text_View_Bold>
                        <LinearLayout
                            android:gravity="right"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.2"
                            android:layout_gravity="center">
                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/edit_black_48dp"></ImageView>
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.opensis.others.customtext.Custom_Text_View
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/medium_text"
                        android:textColor="@color/bar"
                        android:text="Online Classroom Password"></com.opensis.others.customtext.Custom_Text_View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp">
                        <com.opensis.others.customtext.Custom_Text_View_Bold
                            android:id="@+id/tvOnlinePassword"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/large_text"
                            android:textColor="@color/black"
                            android:text="axCbghjk"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        <LinearLayout
                            android:gravity="right"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.2"
                            android:layout_gravity="center">
                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/file_copy_black_twotone_48dp"></ImageView>
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="20dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/llwhitestrokedesign"
            android:backgroundTint="@color/white">
            <com.opensis.others.customtext.Custom_Text_View_Bold
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/big_text"
                android:textColor="@color/missing_attendance"
                android:text="Duration"></com.opensis.others.customtext.Custom_Text_View_Bold>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <com.opensis.others.customtext.Custom_Text_View
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/medium_text"
                    android:textColor="@color/bar"
                    android:text="Marking Period"></com.opensis.others.customtext.Custom_Text_View>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp">
                    <com.opensis.others.customtext.Custom_Text_View_Bold
                        android:textStyle="bold"
                        android:id="@+id/tvQuater"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/large_text"
                        android:layout_marginTop="2dp"
                        android:textColor="@color/black"
                        ></com.opensis.others.customtext.Custom_Text_View_Bold>
                    <com.opensis.others.customtext.Custom_Text_View
                        android:layout_marginLeft="2dp"
                        android:id="@+id/tvQuaterPeriod"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/large_text"
                        android:layout_marginTop="2dp"
                        android:textColor="@color/black"
                       ></com.opensis.others.customtext.Custom_Text_View>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="20dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/llwhitestrokedesign"
            android:backgroundTint="@color/white">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.opensis.others.customtext.Custom_Text_View_Bold
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/big_text"
                    android:textColor="@color/missing_attendance"
                    android:text="Schedule Type"></com.opensis.others.customtext.Custom_Text_View_Bold>
                <com.opensis.others.customtext.Custom_Text_View
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/big_text"
                    android:textColor="@color/black"
                    ></com.opensis.others.customtext.Custom_Text_View>
            </LinearLayout>

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_marginTop="20dp"
               android:orientation="horizontal">
               <LinearLayout
                   android:layout_width="0dp"
                   android:layout_height="wrap_content"
                   android:layout_weight="1"
                   android:orientation="vertical">
                   <LinearLayout
                       android:layout_marginTop="20dp"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="vertical">
                       <com.opensis.others.customtext.Custom_Text_View
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:textSize="@dimen/medium_text"
                           android:textColor="@color/bar"
                           android:text="Room"></com.opensis.others.customtext.Custom_Text_View>
                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="2dp">
                           <com.opensis.others.customtext.Custom_Text_View
                               android:id="@+id/tvRoom"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textSize="@dimen/large_text"
                               android:layout_marginTop="2dp"
                               android:textColor="@color/black"
                               ></com.opensis.others.customtext.Custom_Text_View>
                       </LinearLayout>

                   </LinearLayout>
               </LinearLayout>
               <LinearLayout
                   android:layout_width="0dp"
                   android:layout_height="wrap_content"
                   android:layout_weight="1"
                   android:orientation="vertical">
                   <LinearLayout
                       android:layout_marginTop="20dp"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="vertical">
                       <com.opensis.others.customtext.Custom_Text_View
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:textSize="@dimen/medium_text"
                           android:textColor="@color/bar"
                           android:text="Period"></com.opensis.others.customtext.Custom_Text_View>
                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="2dp">
                           <com.opensis.others.customtext.Custom_Text_View
                               android:id="@+id/tvPeriod"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textSize="@dimen/large_text"
                               android:layout_marginTop="2dp"
                               android:textColor="@color/black"
                              ></com.opensis.others.customtext.Custom_Text_View>
                       </LinearLayout>

                   </LinearLayout>
               </LinearLayout>
           </LinearLayout>
            <LinearLayout
