                        android:layout_height="wrap_content"
                        android:id="@+id/lnWeekDays"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/lnSun"
                            android:gravity="center"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/llgreystrokedesign">
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvSun"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="U"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="2dp"
                            android:id="@+id/lnMon"
                            android:gravity="center"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:background="@drawable/llgreystrokedesign">
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:id="@+id/tvMon"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
