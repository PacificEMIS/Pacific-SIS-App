                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.opensis.others.customtext.Custom_Text_View
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/medium_text"
                        android:textColor="@color/bar"
                        android:text="Course Section Name"></com.opensis.others.customtext.Custom_Text_View>
                    <com.opensis.others.customtext.Custom_Text_View_Bold
                        android:id="@+id/tvCourseSectionName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/large_text"
                        android:layout_marginTop="2dp"
                        android:textColor="@color/black"
                       ></com.opensis.others.customtext.Custom_Text_View_Bold>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
