<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginBottom="20dp">
    <com.opensissas.others.customtext.Custom_Text_View_SemiBold
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/medium_text"
        android:textColor="@color/black"
        android:id="@+id/tvGradeName"></com.opensissas.others.customtext.Custom_Text_View_SemiBold>
    <com.opensissas.others.customtext.Custom_Text_View
        android:layout_marginTop="2dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/small_text"
        android:textColor="@color/missing_attendance"
       android:text="Total not 100%"
        android:id="@+id/tvQtrHundred"></com.opensissas.others.customtext.Custom_Text_View>
    <LinearLayout
        android:id="@+id/lnQtrGrade"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="@drawable/llgreystrokedesign"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:paddingRight="20dp"
        android:orientation="vertical">
        <com.opensissas.others.customtext.Custom_Text_View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/bar"
            android:id="@+id/tvGradeTotal"></com.opensissas.others.customtext.Custom_Text_View>
        <com.opensissas.others.customtext.Custom_EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            android:textColor="@color/black"
            android:inputType="number"
            android:textSize="@dimen/medium_text"
            android:id="@+id/etGradeTotal"></com.opensissas.others.customtext.Custom_EditText>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/lnQtrExam"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="@drawable/llgreystrokedesign"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:paddingRight="20dp"
        android:orientation="vertical">
        <com.opensissas.others.customtext.Custom_Text_View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/bar"
            android:id="@+id/tvGradeExam"></com.opensissas.others.customtext.Custom_Text_View>
        <com.opensissas.others.customtext.Custom_EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            android:textColor="@color/black"
            android:inputType="number"
            android:textSize="@dimen/medium_text"
            android:id="@+id/etGradeExam"></com.opensissas.others.customtext.Custom_EditText>
    </LinearLayout>

</LinearLayout>