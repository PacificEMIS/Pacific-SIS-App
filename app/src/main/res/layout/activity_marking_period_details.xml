<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background_color"
    tools:context=".ui.teacher.activity.MarkingPeriodDetailsActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:background="@drawable/twosidecurveheader"
                    android:padding="24dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imgBack"
                            android:layout_width="34dp"
                            android:layout_height="34dp"
                            android:src="@drawable/arrow_back_white_48dp"></ImageView>
                        <com.opensis.others.customtext.Custom_Text_View_Bold
                            android:id="@+id/tvPeriodName"
                            android:layout_marginLeft="20dp"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/white"
                            android:textSize="@dimen/big_text"></com.opensis.others.customtext.Custom_Text_View_Bold>
                    </LinearLayout>
                   >




                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/llgreystrokedesign"
                    android:backgroundTint="@color/white"
                    android:layout_marginTop="110dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:padding="10dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <com.opensis.others.customtext.Custom_Text_View
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/medium_text"
                            android:textColor="@color/bar"
                            android:text="Title"></com.opensis.others.customtext.Custom_Text_View>

                        <com.opensis.others.customtext.Custom_Text_View_Bold
                            android:id="@+id/tvTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:text="Title"
                            android:textColor="@color/black"
                            android:textSize="@dimen/large_text"></com.opensis.others.customtext.Custom_Text_View_Bold>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp">
                        <LinearLayout
                            android:id="@+id/lnGraded"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:backgroundTint="@color/black"
                            android:background="@drawable/llgreystrokedesign">
                            <ImageView
                                android:layout_gravity="center"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/check_black_48dp"
                                app:tint="@color/white"></ImageView>
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/small_text"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:text="Graded"
                                android:layout_marginLeft="5dp"></com.opensis.others.customtext.Custom_Text_View>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="5dp"
                            android:id="@+id/lnExam"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:backgroundTint="@color/black"
                            android:background="@drawable/llgreystrokedesign">
                            <ImageView
                                android:layout_gravity="center"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/check_black_48dp"
                                app:tint="@color/white"></ImageView>
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/small_text"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:text="Exam"
                                android:layout_marginLeft="5dp"></com.opensis.others.customtext.Custom_Text_View>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginLeft="5dp"
                            android:id="@+id/lnComment"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:backgroundTint="@color/black"
                            android:background="@drawable/llgreystrokedesign">
                            <ImageView
                                android:layout_gravity="center"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/check_black_48dp"
                                app:tint="@color/white"></ImageView>
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/small_text"
                                android:textColor="@color/white"
                                android:layout_gravity="center"
                                android:text="Comment"
                                android:layout_marginLeft="5dp"></com.opensis.others.customtext.Custom_Text_View>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="20dp">
                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Short Name"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:layout_marginTop="5dp"
                                android:id="@+id/tvShortName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:textColor="@color/black"
                                android:text="Title"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="20dp">
                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Begins"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:layout_marginTop="5dp"
                                android:id="@+id/tvBegin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:textColor="@color/black"
                                android:text="Title"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Ends"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:layout_marginTop="5dp"
                                android:id="@+id/tvEnd"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:textColor="@color/black"
                                android:text="Title"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="20dp">
                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Grade Posting Begins"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:layout_marginTop="5dp"
                                android:id="@+id/tvPostingBegin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:textColor="@color/black"
                                android:text="Title"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <com.opensis.others.customtext.Custom_Text_View
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/medium_text"
                                android:textColor="@color/bar"
                                android:text="Grade Posting Ends"></com.opensis.others.customtext.Custom_Text_View>
                            <com.opensis.others.customtext.Custom_Text_View_Bold
                                android:layout_marginTop="5dp"
                                android:id="@+id/tvPostingEnd"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/large_text"
                                android:textColor="@color/black"
                                android:text="Title"></com.opensis.others.customtext.Custom_Text_View_Bold>
                        </LinearLayout>
                    </LinearLayout>



                </LinearLayout>


            </FrameLayout>
        </LinearLayout>




    </LinearLayout>
</LinearLayout>